import java.util.TreeSet;

class Produto implements Comparable<Produto> {
    private String nome;
    private double preco;

    public Produto(String nome, double preco) {
        this.nome = nome;
        this.preco = preco;
    }

    @Override
    public int compareTo(Produto outro) {
        // Ordenação por preço (menor para o maior)
        return Double.compare(this.preco, outro.preco);
    }

    @Override
    public String toString() {
        return nome + " (R$ " + preco + ")";
    }
}

public class TesteTreeSetPersonalizado {
    public static void main(String[] args) {
        TreeSet<Produto> produtos = new TreeSet<>();

        produtos.add(new Produto("Teclado", 150.0));
        produtos.add(new Produto("Mouse", 80.0));
        produtos.add(new Produto("Monitor", 900.0));

        System.out.println("Produtos ordenados por preço:");
        for (Produto p : produtos) {
            System.out.println(p);
        }
    }
}
